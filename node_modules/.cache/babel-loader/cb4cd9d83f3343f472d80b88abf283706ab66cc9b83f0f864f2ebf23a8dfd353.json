{"ast":null,"code":"import { defineComponent } from 'vue';\nimport FinanceF from '@/components/FinanceF.vue';\nexport default defineComponent({\n  components: {\n    FinanceF\n  },\n  data() {\n    return {\n      selectedTimeframe: 'weekly',\n      series: [],\n      totalSum: 0,\n      options: {\n        title: {\n          text: 'Savings Growth',\n          fontSize: '40px',\n          align: 'left'\n        },\n        chart: {\n          id: 'apex-radial-bar'\n        },\n        colors: ['#2E7D32', '#4CAF50', '#8BC34A', '#CDDC39', '#FFEB3B', '#FFC107'],\n        markers: {\n          size: 4,\n          hover: {\n            sizeOffset: 6\n          }\n        },\n        labels: ['Education', 'Health', 'Recreation', 'Food', 'Miscellaneous'],\n        legend: {\n          show: true,\n          position: 'bottom',\n          horizontalAlign: 'center',\n          markers: {\n            fillColors: ['#2E7D32', '#4CAF50', '#8BC34A', '#CDDC39', '#FFEB3B', '#FFC107']\n          }\n        },\n        plotOptions: {\n          radialBar: {\n            hollow: {\n              size: '50%'\n            },\n            dataLabels: {\n              name: {\n                offsetY: -21,\n                show: true,\n                color: '#888',\n                fontSize: '30px'\n              },\n              value: {\n                offsetY: 0,\n                fontSize: '40px',\n                color: '#111',\n                formatter: function (val) {\n                  return val;\n                }\n              },\n              total: {\n                show: true,\n                label: 'TOTAL',\n                color: '#888',\n                formatter: function (w) {\n                  return `${w.globals.seriesTotals.reduce((a, b) => a + b, 0)}`;\n                }\n              }\n            }\n          }\n        },\n        fill: {\n          colors: ['#2E7D32', '#4CAF50', '#8BC34A', '#CDDC39', '#FFEB3B', '#FFC107']\n        }\n      }\n    };