{"ast":null,"code":"import ChatWindow from '@/components/ChatWindow.vue';\nimport axios from 'axios';\nimport authService from '../path-to-your-auth-service'; // Import your authentication service\n\nexport default {\n  components: {\n    ChatWindow\n  },\n  data() {\n    return {\n      messages: [],\n      newMessage: '',\n      userFirstName: '' // Store the user's Firstname\n    };\n  },\n\n  methods: {\n    async sendMessage() {\n      try {\n        // Get the user's Firstname from the authentication service\n        this.userFirstName = authService.getUserFirstName();\n\n        // Perform the Axios POST request to send the message\n        await axios.post('sendMessage', {\n          message: this.newMessage,\n          sender: this.userFirstName\n        });\n\n        // Assuming you want to fetch updated messages after sending\n        await this.fetchMessages();\n\n        // Optionally, you can emit an event to inform parent components\n        this.$emit('message-sent', this.newMessage);\n\n        // Clear the input field\n        this.newMessage = '';\n      } catch (error) {\n        console.error('Error sending message:', error);\n      }\n    },\n    async fetchMessages() {\n      try {\n        const response = await axios.get('getMessage');\n        this.messages = response.data;\n      } catch (error) {\n        console.error('Error fetching messages:', error);\n      }\n    }\n  },\n  mounted() {\n    this.fetchMessages();\n  }\n};","map":{"version":3,"names":["ChatWindow","axios","authService","components","data","messages","newMessage","userFirstName","methods","sendMessage","getUserFirstName","post","message","sender","fetchMessages","$emit","error","console","response","get","mounted"],"sources":["C:\\Users\\user\\OneDrive\\Desktop\\HiyasProject\\src\\views\\ChatView.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <chat-window :messages=\"messages\" />\r\n    <input v-model=\"newMessage\" />\r\n    <button @click=\"sendMessage\">Send</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ChatWindow from '@/components/ChatWindow.vue';\r\nimport axios from 'axios';\r\nimport authService from '../path-to-your-auth-service'; // Import your authentication service\r\n\r\nexport default {\r\n  components: {\r\n    ChatWindow,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      messages: [],\r\n      newMessage: '',\r\n      userFirstName: '', // Store the user's Firstname\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    async sendMessage() {\r\n      try {\r\n        // Get the user's Firstname from the authentication service\r\n        this.userFirstName = authService.getUserFirstName();\r\n\r\n        // Perform the Axios POST request to send the message\r\n        await axios.post('sendMessage', {\r\n          message: this.newMessage,\r\n          sender: this.userFirstName,\r\n        });\r\n\r\n        // Assuming you want to fetch updated messages after sending\r\n        await this.fetchMessages();\r\n\r\n        // Optionally, you can emit an event to inform parent components\r\n        this.$emit('message-sent', this.newMessage);\r\n\r\n        // Clear the input field\r\n        this.newMessage = '';\r\n      } catch (error) {\r\n        console.error('Error sending message:', error);\r\n      }\r\n    },\r\n\r\n    async fetchMessages() {\r\n      try {\r\n        const response = await axios.get('getMessage');\r\n        this.messages = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching messages:', error);\r\n      }\r\n    },\r\n  },\r\n\r\n  mounted() {\r\n    this.fetchMessages();\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AASA,OAAOA,UAAS,MAAO,6BAA6B;AACpD,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,WAAU,MAAO,8BAA8B,EAAE;;AAExD,eAAe;EACbC,UAAU,EAAE;IACVH;EACF,CAAC;EAEDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,EAAE;MACdC,aAAa,EAAE,EAAE,CAAE;IACrB,CAAC;EACH,CAAC;;EAEDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI;QACF;QACA,IAAI,CAACF,aAA